@model IEnumerable<cjstores.API.Data.Entities.TipoDocumento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre_TD)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Abreviatura)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre_TD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Abreviatura)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_TD">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_TD">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_TD">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
