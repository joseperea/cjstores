@model cjstores.API.Data.Entities.TipoDocumento

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar</h1>

<h4>Tipo de Documento</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id_TD" />
            <partial name="_TipoDocumento" model="@Model" />
            <div class="form-group">
                <input type="submit" value="Editar" class="btn btn-outline-success" />
                <a asp-action="Index" class="btn btn-outline-dark">Regresar</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
